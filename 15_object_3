const course={
    course:"js",
    price:"8393",
    teacher:"bablu"
}
 // object destructure
const {teacher}=course // extract kr rhe h agli bar jab bhi teacher likhenge teacher ki value aayegi 
console.log(teacher)//bablu
const{course:c}=course //-> name ko bhi change kar skte h 
console.log(c) //js


